<section class="form-auth text-center">
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <%# <img class="mb-4" src="/docs/4.3/assets/brand/bootstrap-solid.svg" alt="" width="72" height="72"> %>
  <h1 class="h3 mb-3 font-weight-normal">Please Log in</h1>
  <label for="inputEmail" class="sr-only">Email address</label>
  <%= f.email_field :email, autofocus: true, class: "form-control", id: "inputEmail", placeholder: "Email address",  autocomplete: "current-email"  %>
  <label for="inputPassword" class="sr-only">Password</label>
  <%= f.password_field :password, class: "form-control", id: "inputPassword", placeholder: "Password",  autocomplete: "current-password"  %>
  <div class="checkbox mb-3">
    <label>
      <input type="checkbox" value="remember-me"> Remember me
    </label>
  </div>
  <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
  <p class="mt-5 mb-3 text-muted">Â© 2023</p>
<% end %>

<%= render "devise/shared/links" %>

</section>
